
{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vscode"]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": ["node_modules", ".vscode-test"]
}
    